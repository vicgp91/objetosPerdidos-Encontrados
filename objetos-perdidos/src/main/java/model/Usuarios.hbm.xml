<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="model.Usuarios" table="usuarios" catalog="objetosperdidos" optimistic-lock="version">
        <id name="username" type="string">
            <column name="USERNAME" length="50" />
            <generator class="assigned" />
        </id>
        <property name="pass" type="string">
            <column name="PASS" length="50" />
        </property>
        <property name="nombreCompleto" type="string">
            <column name="NOMBRE_COMPLETO" length="50" />
        </property>
        <property name="perfil" type="string">
            <column name="PERFIL" length="5" />
        </property>
        <property name="ultimoIngreso" type="timestamp">
            <column name="ULTIMO_INGRESO" length="19" />
        </property>
        <property name="correoElectronico" type="string">
            <column name="CORREO_ELECTRONICO" length="30" />
        </property>
    </class>
</hibernate-mapping>